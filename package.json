{
  "name": "stream-f36",
  "description": "Experiments in Node streams and Forma 36",
  "scripts": {
    "postinstall": "npm install sqlite3",
    "start": "run-p start:client start:server",
    "start:client": "parcel serve --no-cache --port 8080 static/index.html",
    "start:server": "damien -r dotenv/config api/server.js",
    "start:server:inspect": "damien --inspect-brk -r dotenv/config api/server.js",
    "_build": "npm run build:client && npm run build:server",
    "build:client": "parcel build --no-cache --no-source-maps static/index.html -d public",
    "build:server": "parcel build --no-source-maps -t node -d api -o server server/index.js"
  },
  "author": "Manuel Spagnolo <spagnolo.manu@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@contentful/forma-36-fcss": "0.0.32",
    "@contentful/forma-36-react-components": "^3.24.0",
    "@contentful/forma-36-tokens": "^0.4.5",
    "@types/classnames": "^2.2.9",
    "@types/node": "^12.12.14",
    "@types/react": "^16.9.15",
    "@types/react-router-dom": "^5.1.3",
    "@types/recharts": "^1.8.3",
    "autoprefixer": "^9.7.3",
    "classnames": "^2.2.6",
    "contentful": "^7.10.0",
    "damien": "^1.0.3",
    "dotenv": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "postcss-modules": "^1.4.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "recharts": "^2.0.0-beta.1",
    "sass": "^1.23.7",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.0.2",
    "sequelize": "^5.21.2",
    "sqlite3": "^4.1.1"
  },
  "browserslist": [
    "> 5%"
  ]
}
